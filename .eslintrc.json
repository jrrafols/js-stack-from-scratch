{
    "env":{
        "browser": true
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "arrow-body-style": [0, "always"],
        "new-cap": [
            2,
            {
                "capIsNewExceptions": [
                    "Map",
                    "List"
                ]
            }
        ]
    },
    "extends": "airbnb",
    "plugins": [
        "import"
    ]
}